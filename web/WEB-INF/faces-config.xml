<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
    xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    <navigation-rule>
        <from-view-id>/staff/productListView.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>productentryform</from-outcome>
            <to-view-id>/staff/productentryform.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>update</from-outcome>
            <to-view-id>/staff/update.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/staff/productentryform.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>productentryform</from-outcome>
            <to-view-id>/staff/productentryform.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/staff/update.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>productListView</from-outcome>
            <to-view-id>/staff/productListView.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/public/cart.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>cart</from-outcome>
            <to-view-id>/public/cart.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>productViewCustomer</from-outcome>
            <to-view-id>/public/productViewCustomer.xhtml</to-view-id>
             <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>checkout</from-outcome>
            <to-view-id>/customer/checkout.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/customer/checkout.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>customerOrder</from-outcome>
            <to-view-id>/customer/customerOrder.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/customer/customerOrder.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>confirmation</from-outcome>
            <to-view-id>/customer/confirmation.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/staff/purchaseForm.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>purchaseForm</from-outcome>
            <to-view-id>/staff/purchaseForm.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/public/registration.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>success</from-outcome>
            <to-view-id>/login.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>fail</from-outcome>
            <to-view-id>/public/registration.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/public/productViewCustomer.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>show</from-outcome>
            <to-view-id>/public/DetailedProduct.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/admin/EmployeeRegistration.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>success</from-outcome>
            <to-view-id>/admin/success.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>unsuccess</from-outcome>
            <to-view-id>/admin/EmployeeRegistration.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
      <navigation-rule>
        <from-view-id>/public/bannar.xhtml</from-view-id>
      
        <navigation-case>
            <from-outcome>index</from-outcome>
            <to-view-id>/public/index.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        
    </navigation-rule>
</faces-config>
